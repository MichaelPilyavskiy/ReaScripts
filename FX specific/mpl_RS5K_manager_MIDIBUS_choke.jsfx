@init

@block
  sliderID = 1;
  note = 0;
  loop(64, 
    noteflags[note] = $xFF & floor(slider(sliderID));
    noteflags[note+1] = $xFF & floor((slider(sliderID)>>8));
    sliderID += 1;
    note +=2;
  );
  
  
  while ( midirecv(msgoffs,msg1,msg2,msg3)  ) ( 
    msgchan = msg1 & $xF; 
    currentnote_msgtype=(msg1>>4)  & $xF; //current note
    currentnote_msgtype == 9 ? //is note ON
    (
      test0 = noteflags[42];
      currentnote_chokeflags = noteflags[msg2];
      note = -1;
      loop(127, // loop notes, close them if current incoming note has active choke group
        note +=1;
        chokegroup = 0;
        looped_noteflag = noteflags[note];
        chokegroup = 0;
        loop(8, // get current note choke group flags
          chokegroup+=1;
          chokegroupactive_incnote = (currentnote_chokeflags>>(chokegroup-1)) & 1;
          chokegroupactive_loopednote = (looped_noteflag>>(chokegroup-1)) & 1;
          (chokegroupactive_incnote == 1 && chokegroupactive_loopednote == 1 ) ? (
            midisend(msgoffs,0x80|msgchan,note);
            test1 = currentnote_chokeflags;
            test1a = chokegroupactive_incnote;
            test2 = looped_noteflag;
            test3= note  
          );
          
         ); 
        
      );
    );
    
    // pass through
    midisend(offset,msg1,msg2,msg3);
  );